#!/bin/bash

#export PYTHONPATH="/home_directory/.local/lib/python3.5/site-packages/:/usr/local/lib/python3.5/dist-packages/"
export PYTHONPATH=""

m=20000
dataset=cifar
net=vgg16
L=4
prefix=msweep
sigmaw=2.0
sigmab=0.1
pool=avg
c=0.0

n_gpus=1
export n_gpus=$n_gpus

  ./run_experiment --prefix $prefix --m $m --dataset $dataset --network $net --number_layers $L --training --sigmaw $sigmaw --sigmab $sigmab --confusion $c --n_gpus $n_gpus --pooling $pool --n_samples_repeats 2.0 --use_empirical_K
